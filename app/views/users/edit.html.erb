<h1><%= @mypage.name %>の編集</h1>

<%= form_tag("/mypage_update/#{@mypage.id}", {multipart: true}) do %>
  
  <div>画像</div>
  <input name="image" type="file">

  <div>ユーザー名</div>
  <input name="name" value="<%=@mypage.name%>">

  <div>ID</div>
  <input name="email" value="<%=@mypage.email%>">
  
  <div>パスワード<div>
  <input name="password" value="<%=@mypage.password%>">
  
  <input type="submit" value="編集">
<% end %>